        @foreach( var inv in listInv)
        {
<tr>
    <td>@inv.InventoryName</td>
    <td>@inv.Quantity</td>
    <td>@inv.Price</td>
    <td>
        <button type="button" class="btn btn-light"></button>
    </td>
</tr>
         }
@code {
    [Parameter]
    public List<Inventory> listInv { get; set; } = new List<Inventory>();

}
